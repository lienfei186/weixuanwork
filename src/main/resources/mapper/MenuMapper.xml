<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cn.weixuan.dao.MenuMapper">
    <insert id="saveMenu" parameterType="com.cn.weixuan.pojo.Menu">
        INSERT INTO sys_menu
        (menuName,permission,resourceType,parentId,parentName,url) VALUES
        (#{menuName},#{permission},#{resourceType},#{parentId},#{parentName},#{url})
    </insert>
</mapper>
