<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cn.weixuan.dao.AdressMapper">

    <insert id="insertAdress" parameterType="com.cn.weixuan.pojo.Adress">
        INSERT INTO `user_address`(`userName`, `state`, `userAdress`, `userarea`)
        VALUES (#{userName}, #{state}, #{userAdress}, #{userarea});
    </insert>
</mapper>